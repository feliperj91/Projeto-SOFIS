{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "outputDirectory": ".",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}